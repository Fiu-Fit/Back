{
  "name": "@fiu-fit/service-registry",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:migrate": "prisma migrate dev",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "sudo docker compose rm db -s -f -v",
    "db:dev:up": "sudo docker compose up db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && (sleep 1 || timeout 1) && yarn prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && (sleep 1 || timeout 1) && yarn prisma:test:deploy",
    "purge": "rimraf node_modules && rimraf dist",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn db:dev:up && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --no-cache --config ./test/jest-e2e.json",
    "common:install": "yarn add @fiu-fit/common@git@github.com:Fiu-Fit/common.git && yarn"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seeder.ts"
  },
  "dependencies": {
    "@fiu-fit/common": "git@github.com:Fiu-Fit/common.git",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.3.12",
    "@nestjs/platform-express": "^9.3.12",
    "@prisma/client": "^4.12.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "generate-api-key": "^1.0.2",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.1",
    "webpack": "^5.82.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
